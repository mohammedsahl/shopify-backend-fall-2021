<h1 class="display-4 mb-4">All uploaded images</h1>
<table class="table table-hover">
    <thead class="thead-dark">
        <tr>
            <th>Title</th>
            <th>Tags</th>
            <th>Image</th>
            <th></th>
        </tr>
    </thead>
    <tbody>
        <% images.forEach((image)=> { %>
            <tr>
                <td><a target="_blank" href="/images/uploads/<%= image.imageFilename %>"><%= image.imageTitle %></a></td>
                <td><%= image.imageTags %></td>
                <td><img width="100%" src="/images/uploads/<%= image.imageFilename %>"></td>
                <td>
                    <form action="/images/delete/<%= image.id %>" method="post">
                        <input type="submit" value="ðŸ—‘" class="btn btn-dark">
                    </form>
                </td>
            </tr>
        <% }) %>
    </tbody>
</table>