<h1 class="display-4">List of all images</h1>
<table class="table table-striped">
    <thead>
        <tr>
            <th>#</th>
            <th>Title</th>
            <th>Tags</th>
            <th>Directory</th>
            <th>Image</th>
        </tr>
    </thead>
    <tbody>
        <% images.forEach((image) => { %>
            <tr>
                <td><%= image.id %></td>
                <td><%= image.imageTitle %></td>
                <td><%= image.imageTags %></td>
                <td><%= image.imageUploadFileDirectory %></td>
                <td><img width="150px" src="/images/uploads/<%= image.imageFilename %>"></td>
            </tr>
        <% }) %>
    </tbody>
</table>